{
  "name": "crush-lu",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -i ./static/crush_lu/css/tailwind-input.css -o ./static/crush_lu/css/tailwind.css --minify",
    "watch:css": "tailwindcss -i ./static/crush_lu/css/tailwind-input.css -o ./static/crush_lu/css/tailwind.css --watch",
    "build:css:power-up": "tailwindcss -i ./static/power_up/css/tailwind-input.css -o ./static/power_up/css/tailwind.css --minify",
    "watch:css:power-up": "tailwindcss -i ./static/power_up/css/tailwind-input.css -o ./static/power_up/css/tailwind.css --watch",
    "build:css:tableau": "tailwindcss -i ./static/tableau/css/tailwind-input.css -o ./static/tableau/css/tailwind.css --minify",
    "watch:css:tableau": "tailwindcss -i ./static/tableau/css/tailwind-input.css -o ./static/tableau/css/tailwind.css --watch",
    "build:css:all": "npm run build:css && npm run build:css:power-up && npm run build:css:tableau"
  },
  "dependencies": {
    "alpinejs": "^3.13.3",
    "htmx.org": "^2.0.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.19",
    "tailwindcss": "^4.1.18"
  }
}
